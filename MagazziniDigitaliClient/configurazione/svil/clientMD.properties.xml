<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties version="1.0">
    <comment>Fie di configurazione del progetto MD Versione 2.x.x</comment>

    <!-- URL da contattare per poter fare l'autenticazione del Software -->
    <entry key="software.URLAuthentication">http://localhost:8080/MagazziniDigitaliServices/services/AuthenticationSoftwarePort?wsdl</entry>

    <!-- Login del Software -->
    <!-- 
    <entry key="software.TD.login">TD_MD</entry>
     -->
    <entry key="software.TD.login">TD_AMM</entry>

    <!-- Percorsi locali in cui vengono registrati la lista dei file da elaborare (Obbligatori almeno 1 percorso)-->
    <!-- 
    <entry key="pathExcel(0)">/mnt/volume1/Locale/Excel</entry>
    <entry key="pathExcel(0)">/mnt/volume1/Bagit/Coda</entry>
    <entry key="pathExcel(0)">/mnt/volume1/Sviluppo/Coda</entry>
     -->
    <entry key="pathExcel(0)">/mnt/Storage/Scambio/Amm/Coda</entry>
    
    <!-- Percorsi locali in cui verranno registrati gli oggetti Digitali da inviare al Magazzino digitale (Obbligatori almeno 1 persorso)-->
    <!-- 
    <entry key="pathDescriptati(0)">/mnt/volume1/Locale/Decompresso</entry>
    <entry key="pathDescriptati(0)">/mnt/volume1/Bagit/Tar</entry>
    <entry key="pathDescriptati(0)">/mnt/volume1/Sviluppo/Tgz</entry>
     -->
    <entry key="pathDescriptati(0)">/mnt/Storage/Scambio/Amm/Tgz</entry>

    <entry key="md.sendRsync.path">/opt/local/bin/rsync</entry>

    <entry key="md.deleteLocalFile">true</entry>
    
</properties>